    "heroku-postbuild": "npm run client-install-deps && npm run client-build"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "stripe": "^8.49.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.2"
  }
}